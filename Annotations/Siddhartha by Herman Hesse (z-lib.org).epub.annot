    <annotation>
        <dc:identifier>urn:uuid:0a7c469d-3d27-4282-9cf9-d22578df1f68</dc:identifier>
        <dc:date>2022-02-03T06:34:43Z</dc:date>
        <dc:creator>urn:uuid:21efe10b-8d53-4fd1-8d2e-8909a23bfee6</dc:creator>
        <target>
            <fragment start="temp_calibre_txt_input_to_html.html#point(/1/4/340/1:813)" end="temp_calibre_txt_input_to_html.html#point(/1/4/340/1:833)" progress="0.283951">
                <text>his self was Atman, </text>
            </fragment>
        </target>
    </annotation>
    <annotation>
        <dc:identifier>urn:uuid:57c5c29d-1927-40c4-bd7f-2b46af5c3f6d</dc:identifier>
        <dc:date>2022-02-03T06:38:03Z</dc:date>
        <dc:creator>urn:uuid:21efe10b-8d53-4fd1-8d2e-8909a23bfee6</dc:creator>
        <target>
            <fragment start="temp_calibre_txt_input_to_html.html#point(/1/4/340/1:1811)" end="temp_calibre_txt_input_to_html.html#point(/1/4/340/1:1865)" progress="0.283951">
                <text>except for what the voice commanded him to strive for,</text>
            </fragment>
        </target>
        <content>
            <dc:date>2022-02-03T06:38:03Z</dc:date>
